{
    "bootstrap": "vendor/autoload.php",
    "path": "tests/Bytemap/Performance",
    "reports": {
    	"time": {
    		"extends": "aggregate",
    		"cols": ["subject", "set", "mode", "mean", "rstdev", "diff"]
    	},
    	"memory": {
	        "extends": "aggregate",
	        "cols": ["subject", "set", "mem_real", "mem_final", "mem_peak", "diff"],
	        "diff_col": "mem_final"
	    },
	    "all": {
	    	"generator": "composite",
	    	"reports": ["time", "memory"]
	    }
    }
}