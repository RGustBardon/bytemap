{
    "name": "bytemap/bytemap",
    "description": "PHP data structures with low memory footprint",
	"keywords": ["map", "mapping", "structure"],
    "homepage": "https://github.com/RGustBardon/bytemap",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Gust-Bardon",
            "email": "robert@gust-bardon.org",
            "homepage": "https://robert.gust-bardon.org"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "conflict": {
        "hhvm": "*"
    },
    "require-dev": {
        "ext-SPL": "*",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "php-coveralls/php-coveralls": "^2.1",
        "php-ds/php-ds": "^1.2",
        "phpunit/phpunit": "^7.2",
        "salsify/json-streaming-parser": "^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "salsify/json-streaming-parser": "For instantiating bytemaps from JSON with low memory footprint"
    },
    "autoload": {
        "psr-4": {"Bytemap\\": "src/Bytemap"}
    },
    "autoload-dev": {
        "psr-4": {"Bytemap\\": "tests/Bytemap"}
    },
    "scripts": {
        "test": [
            "parallel-lint . --exclude vendor",
            "phpunit"
        ]
    }
}